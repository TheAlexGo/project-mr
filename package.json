{
  "name": "mangareader",
  "description": "Лучшее приложение для чтения манги во всём мире. Да что там в мире - в России!",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook -o sb-dist",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint:tslint": "tsc --noEmit",
    "lint:stylelint": "stylelint 'src/**/*.styl'",
    "lint:eslint": "eslint src --ext .ts,.tsx",
    "lint:prettier": "prettier --check src/**/*.{ts,tsx}",
    "prepare": "husky install",
    "lint:stylelint:fix": "stylelint 'src/**/*.styl' --fix",
    "lint:eslint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:prettier:fix": "prettier --write src/**/*.{ts,tsx}",
    "watch:ts": "tsc --noEmit --watch"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "mobx": "^6.7.0",
    "mobx-react-lite": "^3.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-list": "^0.8.17",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.3.0",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-list": "^0.8.7",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-legacy": "^3.0.2",
    "@vitejs/plugin-react": "^3.0.1",
    "axios": "^1.2.6",
    "babel-loader": "^9.1.2",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.3",
    "stylelint": "^14.16.1",
    "stylelint-config-rational-order": "^0.0.4",
    "stylelint-order": "^6.0.1",
    "stylelint-stylus": "^0.17.1",
    "stylus": "^0.59.0",
    "transliteration": "^2.3.5",
    "typescript": "^4.9.4",
    "uuid-int": "^3.1.0",
    "vite": "^4.0.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.14.1",
    "vite-plugin-stylelint": "^4.1.6",
    "vite-plugin-svgr": "^2.4.0"
  }
}
